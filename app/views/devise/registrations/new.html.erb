<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name), :html => { :role => 'form'}) do |f| %>
<%= devise_error_messages! %>
<div class="form-group">
  <%= f.text_field :first_name, :autofocus => true, class: 'form-control', placeholder: 'First Name', label: false %>
</div>
<div class="form-group">
  <%= f.text_field :last_name, class: 'form-control', placeholder: 'Last Name', label: false %>
</div>
<div class="form-group">
  <%= f.text_field :invitation_code, class: 'form-control', placeholder: 'Invitation Code (Optional)', label: false %>
</div>
<div class="form-group">
  <%= f.email_field :email, class: 'form-control', placeholder: 'Email', label: false %>
</div>
<div class="form-group">
  <%= f.password_field :password, class: 'form-control', placeholder: 'Password', label: false %>
</div>
<div class="form-group">
  <%= f.password_field :password_confirmation, class: 'form-control', placeholder: 'Confirmation', label: false %>
</div>
<p>Our <a href="">terms and policy</a></p>
<%= f.submit 'Agree and Sign up', :class => 'btn btn-primary block full-width m-b' %>
<% end %>
<p class="text-muted text-center"><small>Are you a professional?</small></p>
<%= link_to 'Create an account for your practice', '/practice/register', class: 'btn btn-sm btn-white btn-block' %>  

